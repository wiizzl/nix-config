{
  "icon_theme": "Catppuccin Mocha",
  "theme": "Catppuccin Mocha",

  "auto_install_extensions": {
    "html": true,
    "astro": true,
    "nix": true,
    "toml": true,
    "php": true,
    "dockerfile": true,
    "sql": true,
    "vue": true,
    "xml": true,
    "java": true,
    "prisma": true,
    "csharp": true,
    "biome": true,
    "catppuccin": true,
    "catppuccin-icons": true
  },

  "agent": {
    "default_model": {
      "model": "gpt-4o",
      "provider": "copilot_chat"
    },
    "default_profile": "ask",
    "dock": "left",
    "inline_assistant_model": {
      "model": "gpt-4.1",
      "provider": "copilot_chat"
    }
  },
  "diagnostics": {
    "button": false,
    "include_warnings": true,
    "inline": {
      "enabled": true,
      "max_severity": null,
      "min_column": 0,
      "padding": 4,
      "update_debounce_ms": 150
    }
  },
  "indent_guides": {
    "active_line_width": 2,
    "coloring": "indent_aware",
    "enabled": true,
    "line_width": 1
  },
  "project_panel": {
    "dock": "left"
  }
}
